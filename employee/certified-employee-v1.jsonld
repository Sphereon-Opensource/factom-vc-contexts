{
  "@context":
  {
    "@version": 1.1,
    "cec": "https://sphereon-opensource.github.io/factom-vc-contexts/employee/certified-employee-v1#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "schema": "http://schema.org/",

    "CertifiedEmployeeCredential" : "cec:CertifiedEmployeeCredential",
    "Person": "schema:Person",
    "employer": "cec:Employer",
    "employee": "cec:Employee",

    "issueDate": {
      "@id": "cec:issueDate",
      "@type": "xsd:dateTime"
    },
    "expirationDate": {
      "@id": "cec:expirationDate",
      "@type": "xsd:dateTime"
    },
    "Employee": {
      "@id": "cec:Employee",
      "@context": {
        "@version": 1.1,
        "id": "@id",
        "type": ["Employee", "Person"],

        "email": {
          "@id": "cec:email",
          "@type": "schema:email"
        },
        "familyName": "cec:familyName",
        "gender": "cec:gender",
        "givenName": "cec:givenName",
        "birthCountry": "cec:birthCountry",
        "birthDate": {
          "@id": "cec:birthDate",
          "@type": "xsd:dateTime"
        },
        "identityDocumentType": "cec:identityDocumentType",
        "identityDocumentNumber": "cec:identityDocumentNumber",
        "employeeId": "cec:employeeId",
        "department": "cec:department",
        "image": "cec:image"
      }
    },
    "Employer": {
      "@id": "cec:Employer",
      "@context": {
        "@version": 1.1,

        "id": "@id",
        "type": ["Employer", "Person"],

        "name": "cec:name",
        "identificationNumber": "cec:identificationNumber",
        "taxNumber": "cec:taxNumber",
        "address": "cec:address",
        "province": "cec:province",
        "district": "cec:district",
        "country": "cec:country"
      }
    }
  }
}
